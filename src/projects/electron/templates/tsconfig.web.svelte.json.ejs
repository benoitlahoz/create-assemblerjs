{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/*"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/features/*": ["src/features/*"],
      "@/main/*": ["src/main/*"],
      "@/renderer/*": ["src/renderer/src/*"],
      "@/preload/*": ["src/preload/*"],
      "@/windows/*": ["src/windows/*"],
      "@/resources/*": ["resources/*"]
    }
  }
}